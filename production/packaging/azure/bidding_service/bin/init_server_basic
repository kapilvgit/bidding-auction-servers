#!/busybox/sh

set -o errexit
set -x


export GLOG_logtostderr=1
export GLOG_stderrthreshold=0
export GRPC_DNS_RESOLVER=native

# Start the server first.
/server/bin/server --fromenv=buyer_code_fetch_config,enable_bidding_service_benchmark,js_num_workers,js_worker_queue_len,js_worker_mem_mb,port,roma_timeout_ms,test_mode,enable_encryption 

