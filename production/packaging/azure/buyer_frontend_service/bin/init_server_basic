#!/busybox/sh

set -o errexit
set -x

#########################################################################
#                                                                       #
#                             BFE Startup                               #
#                                                                       #
#########################################################################


export GLOG_logtostderr=1
export GLOG_stderrthreshold=0
export GRPC_DNS_RESOLVER=native

# Start the server.
/server/bin/server  --fromenv=bidding_server_addr,buyer_kv_server_addr,enable_buyer_frontend_benchmarking,port,roma_timeout_ms,bidding_signals_load_timeout_ms,test_mode,enable_encryption,generate_bid_timeout_ms
